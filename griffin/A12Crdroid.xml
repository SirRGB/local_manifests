<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="lineage"
		fetch="https://github.com/LineageOS"
		review="review.lineageos.org"
		revision="lineage-19.1" />

	<remote name="legacy"
		fetch="https://github.com/griffin-experimental"
		revision="12.1" />

	<!-- Device repos -->
	<project name="SirRGB/android_device_motorola_griffin" path="device/motorola/griffin" remote="github" revision="lineage-19.1" />
	<project name="SirRGB/android_kernel_motorola_msm8996" path="kernel/motorola/msm8996" remote="github" revision="lineage-19.0" />
	<project name="TheMuppets/proprietary_vendor_motorola" path="vendor/motorola" remote="github" revision="lineage-18.1" />

	<!-- Dependencies -->
	<project name="android_external_bson" path="external/bson" remote="lineage" />
	<project name="android_system_qcom" path="system/qcom" remote="lineage" />

	<!-- DTBtool -->
	<project name="android_system_tools_dtbtool" path="system/tools/dtbtool" remote="lineage" revision="lineage-18.1" />

	<!-- ARM Developer GCC 8.3 kernel toolchain -->
	<project name="chil360/prebuilts_gcc_linux-x86_arm_arm-eabi-8.3" path="prebuilts/gcc/linux-x86/arm/arm-eabi-8.3" remote="github" revision="master" />

	<!-- Patches -->
	<remove-project name="crdroidandroid/android_art" />
	<project name="android_art" path="art" remote="legacy" />

	<remove-project name="platform/system/netd" />
	<project name="android_system_netd" path="system/netd" remote="legacy" />

	<remove-project name="platform/system/bpf" />
	<project name="android_system_bpf" path="system/bpf" remote="legacy" />

	<remove-project name="LineageOS/android_packages_modules_adb" />
	<project name="android_packages_modules_adb" path="packages/modules/adb" remote="legacy" />
</manifest>
